<%= @bodystr_html %>

<% if @seqList %>
	<h3>次の教材を選択してください</h3>
	<dl>
<% @seqList.each do |val| %>
<% i=0 %>
	<dt style="margin:8px 35px;">
		<%= link_to "学習を始める", "/learns/#{val[0]}" %>
		<label>
		<span style="font-weight:bold;font-size:13pt;color:darkred"> ： <%= val[1] %></span>
		</label>
	</dt>
<% i+=1 %>
<% end %>
	</dl>
<% end %>
